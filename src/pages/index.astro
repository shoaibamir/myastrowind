---
import Layout from '~/layouts/PageLayout.astro';

import Hero from '~/components/widgets/Hero.astro';
import Note from '~/components/widgets/Note.astro';
import Features from '~/components/widgets/Features.astro';
import Features2 from '~/components/widgets/Features2.astro';
import Steps from '~/components/widgets/Steps.astro';
import Content from '~/components/widgets/Content.astro';
import BlogLatestPosts from '~/components/widgets/BlogLatestPosts.astro';
import FAQs from '~/components/widgets/FAQs.astro';
import Stats from '~/components/widgets/Stats.astro';
import CallToAction from '~/components/widgets/CallToAction.astro';

const metadata = {
  title: 'No.1 Pakistan Cargo Service in Dubai, Abu Dhabi, Sharjah. We offer sea cargo, air cargo, warehouse storage, customs clearance and packing.',
  ignoreTitleTemplate: true,
};
---

<Layout metadata={metadata}>
  <!-- Hero Widget ******************* -->

  <Hero
    actions={[
      {
        variant: 'primary',
        text: 'Call us',
        href: 'tel:+971504948135',
        target: '_blank',
        icon: 'tabler:phone',
      },
      { text: 'WhatsApp', icon: 'tabler:brand-whatsapp', href: 'https://wa.me/message/TJZP4WU2P2XGD1' },
    ]}
    image={{ src: '~/assets/images/hero.png', alt: 'Pakistan Cargo dubai' }}
  >
    <Fragment slot="title">
      Pakistan <span class="hidden xl:inline"></span>
      <span class="text-accent dark:text-white highlight"> Cargo</span> Dubai
    </Fragment>

    <Fragment slot="subtitle">
      <span class="hidden sm:inline">
        <span class="font-semibold">Door to Door Cargo</span> to Pakistan from Dubai, Abu Dhabi, Sharjah, and other locations. Our door-to-door cargo service ensures that your cargo is picked up from your doorstep and delivered directly to the destination. We offer cargo services in Abu Dhabi and Dubai to Pakistan, ensuring that you have access to our services no matter where you are.
        </span
      >
      <span class="block mb-1 sm:hidden font-bold text-blue-600">No.1 Cargo Servcie for Pakistan.</span>
       Cargo to Pakistan from Dubai, Abu Dhabi, Sharjah.
    </Fragment>
  </Hero>

  <!-- Note Widget ******************* -->

  <Note />

  <!-- Features Widget *************** -->

  <Features
    id="features"
    tagline="Our Services"
    title="Why chose us"
    subtitle="One of the most professional and reliable Pakistani Cargo company in United Arab Emirates from last 10 years."
    items={[
      {
        title: 'Door to Door Cargo',
        description:
          'Safe door to door delivery all over Pakistan using well know business partners include TCS, Leopards couriers.',
        icon: 'tabler:package-export',
      },
      {
        title: 'Free Pickup',
        description:
          'we offer free pickup service all over UAE in our coverage areas.',
        icon: 'tabler:truck',
      },
      {
        title: 'Professional Team',
        description:
          'Our educated and professionl team provide top notch customer servcie.',
        icon: 'tabler:list-check',
      },
      {
        title: 'Special Discount if you lost the job',
        description:
          'We love to help Pakistanis. If you have lost the job and want to send your house holds to Pakistna, we have special discount for you ',
        icon: 'tabler:discount-check',
      },
      {
        title: 'Special Discount for deceased',
        description:
          "Its a big lost but Everyone has to go one day, we provide 50% to 100% dicsount for the cargo of deceased person and if you anyone, let us know to help them.",
        icon: 'tabler:discount-check-filled',
      },
      {
        title: 'Safe and damage free delivery',
        description:
          'We provide 99% damage free door to door shipping servcie provided that if we have done the packing',
        icon: 'tabler:bulb',
      },
    ]}
  />

  <!-- Content Widget **************** -->

  <Content
    isReversed
    tagline="How can we help you"
    title="Shipping and Logistics services"
    items={[
      {
        title: 'Per Kg cargo servcie',
        description:
          'Send box or boxes, furniture, households, cloths, appliances, sofa, decoragtion items on per kg rates.',
      },
      {
        title: 'Full Contaienr Cargo Service',
        description:
          'Want to send 20 feet container or 40 feet container to Pakistan? We are registered freaght broker and provide end to end logistics services.',
      },
      {
        title: 'Special Discount for Move-out customers',
        description:
          'If you are moving out from UAE and want to move all of your households, we can help in transfer of residancy and take care all of shipment need upto your door with special discount.',
      },
    ]}
    image={{
      src: 'https://images.unsplash.com/photo-1594110336951-5bc8c12d6b27?q=80&w=1665&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D',
      alt: 'Colorful Image',
    }}
  >
    <Fragment slot="content">
      <h3 class="text-2xl font-bold tracking-tight dark:text-white sm:text-3xl mb-2">Sea Cargo Service</h3>
       We care and make your deliveries safe and secure.
    </Fragment>

    <Fragment slot="bg">
      <div class="absolute inset-0 bg-blue-50 dark:bg-transparent"></div>
    </Fragment>
  </Content>

  <!-- Content Widget **************** -->

  <Content
    isAfterContent
    items={[
      {
        title: 'Furniture storage',
        description: `Store boxes, extra items, Luggage Storage, Office storage, Furniture storage, House hold storage, Electronics storage.`,
      },
      {
        title: 'Busienss and commercial storage',
        description:
          'Need safe storage space to store your stock, inventry, pallets, textile, retail items or equipments for short or long term? We can take care of all your needs',
      },
      {
        title: 'Packing and Packaging service',
        description:
          "We offer export class packing for International movement which keep all of your goods safe and sound.",
      },
      {
        title: 'International delivery',
        description:
          'We use permium delivery partners for the safe International delivery.',
      },
    ]}
    image={{
      src: 'https://images.unsplash.com/photo-1624137527136-66e631bdaa0e?q=80&w=1887&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D',
      alt: 'Blueprint Image',
    }}
  >
  <Fragment slot="content">
      <h3 class="text-2xl font-bold tracking-tight dark:text-white sm:text-3xl mb-2">Packing and warehosue storage</h3>
      We care and make your deliveries safe and secure.
  </Fragment>

    <Fragment slot="bg">
      <div class="absolute inset-0 bg-blue-50 dark:bg-transparent"></div>
    </Fragment>
  </Content>

  <!-- Content Widget **************** -->

  <Content
    isReversed
    isAfterContent
    items={[
      {
        title: 'Courier Servcies',
        description:
          'We provide International courier services to send documents or parcels using busines partners like DHL, TCS, Fadex, UPS, Skynet and many more.',
      },
      {
        title: 'Transport Services',
        description:
          'Are you looking for local transort service? We provide 1 ton, 3 ton, 7 ton, 10 ton pickups within UAE.',
      },
      {
        title: 'Trailer and Container Transport',
        description:
          'Open trailer, container tranport, curtain side trailer, box trailer, low bed trailer are also availabe.',
      },
      {
        title: 'GCC Transport',
        description: `Want to send cargo to GCC countries? We can help to send your cargo to anywhere in middle east include Saudi Arabia, Bahrai, Qatar, Oman and other Arab Countries.`,
      },
    ]}
    image={{
      src: 'https://images.unsplash.com/photo-1659353740953-c93814e4a2a5?q=80&w=1740&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D',
      alt: 'Astronauts Image',
    }}
  >
      <Fragment slot="content">
        <h3 class="text-2xl font-bold tracking-tight dark:text-white sm:text-3xl mb-2">Courier and Transport</h3>
        We care and make your deliveries safe and secure.
      </Fragment>

    <Fragment slot="bg">
      <div class="absolute inset-0 bg-blue-50 dark:bg-transparent"></div>
    </Fragment>
  </Content>

  <!-- Steps Widget ****************** -->

  <Steps
    title="How to send cargo to Pakistan."
    items={[
      {
        title: 'Step 1: <span class="font-medium">Book an Order</span>',
        description:
          "Call or Whatsapp to book the order, our professional team will schedule the pickup as per availability.",
        icon: 'tabler:package',
      },
      {
        title: 'Step 2: <span class="font-medium">Pickup</em>',
        description:
          "Our professionals will come to your place, proceed to innitial packing to make sure it is safe, wight the cargo and packages, raise the invoice and sent to warehouse for proceed.",
        icon: 'tabler:letter-case',
      },
      {
        title: 'Step 3: <span class="font-medium">Packing and Loading</span>',
        description:
          'Our packers will pack the cargo to make sure it is safe using best availabe packing material and load into the container.',
        icon: 'tabler:paint',
      },
      {
        title: 'Step 4: <span class="font-medium">Shipping and Delivery</span>',
        description:
          'Cargo will be ship to your country and will process local delivery using well know business partners up to your door.',
        icon: 'tabler:check',
      },
    ]}
    image={{
      src: 'https://images.unsplash.com/photo-1616198814651-e71f960c3180?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=987&q=80',
      alt: 'Steps image',
    }}
  />

